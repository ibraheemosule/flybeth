{
  "name": "@packages/shared-frontend",
  "version": "1.0.0",
  "description": "Shared configurations for the FlyBeth monorepo",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./test/server": {
      "types": "./dist/test/server.d.ts",
      "default": "./dist/test/server.js"
    },
    "./test/browser": {
      "types": "./dist/test/browser.d.ts",
      "default": "./dist/test/browser.js"
    },
    "./test/handlers": {
      "types": "./dist/test/handlers.d.ts",
      "default": "./dist/test/handlers.js"
    },
    "./css/*": "./css/*",
    "./vite/*": "./vite/*",
    "./eslint/*": "./eslint/*",
    "./tailwind/*": "./tailwind/*",
    "./vitest/*": "./vitest/*",
    "./postcss/*": "./postcss/*",
    "./tsconfig/*": "./tsconfig/*",
    "./next/*": "./next/*",
    "./msw/*": "./msw/*",
    "./test/*": "./test/*"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "echo \"No tests to run\" && exit 0"
  },
  "dependencies": {
    "@packages/shared-utils": "file:../shared-utils",
    "@hookform/resolvers": "^3.3.2",
    "lucide-react": "^0.294.0",
    "next": "^15.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.66.0",
    "tailwind-merge": "^2.0.0",
    "zustand": "^4.5.5",
    "msw": "^2.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^1.0.4",
    "autoprefixer": "^10.4.16",
    "eslint-config-next": "^14.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "jsdom": "^23.0.1",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.4.14",
    "vite": "^5.4.10",
    "vitest": "^1.0.4",
    "msw": "^2.12.4"
  },
  "files": [
    "dist",
    "eslint",
    "tsconfig",
    "tailwind",
    "vite",
    "postcss",
    "vitest",
    "msw",
    "test",
    "*.d.ts",
    "**/*.d.ts"
  ]
}
